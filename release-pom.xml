    <plugins>
      <plugin>
        <artifactId>maven-war-plugin</artifactId>
        <version>2.1.1</version>
        <extensions>false</extensions>
        <configuration>
          <archive>
            <manifest>
              <addDefaultImplementationEntries>true</addDefaultImplementationEntries>
              <addDefaultSpecificationEntries>true</addDefaultSpecificationEntries>
            </manifest>
          </archive>
          <packagingExcludes>WEB-INF/lib/log4j-*.jar,</packagingExcludes>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>jaxws-maven-plugin</artifactId>
        <version>1.12</version>
        <extensions>false</extensions>
        <executions>
          <execution>
            <id>generate-wsdl</id>
            <goals>
              <goal>wsgen</goal>
            </goals>
          </execution>
        </executions>
        <dependencies>
          <dependency>
            <groupId>com.sun.xml.ws</groupId>
            <artifactId>jaxws-tools</artifactId>
            <version>2.2.6-2</version>
          </dependency>
        </dependencies>
        <configuration>
          <sei>com.rothsmith.demo.ws.HelloWorldWs</sei>
          <genWsdl>true</genWsdl>
          <verbose>true</verbose>
          <sourceDestDir>${project.build.directory}/generated-sources/wsgen</sourceDestDir>
          <destDir>${project.build.directory}/jaxws/wsgen/classes</destDir>
        </configuration>
      </plugin>
    </plugins>
